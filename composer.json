{
    "name": "sicaboy/laravel-http-proxy",
    "description": "A package allows HTTP proxy",
    "keywords": ["http-proxy"],
    "authors": [
        {
            "name": "David Shen",
            "email": "slj@slj.me"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "Sicaboy\\LaravelHttpProxy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sicaboy\\LaravelHttpProxy\\HttpProxyServiceProvider"
            ],
            "aliases": {
                "HttpProxy": "Sicaboy\\LaravelHttpProxy\\Facades\\HttpProxy"
            }
        }
    }
}
